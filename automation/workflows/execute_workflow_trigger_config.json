{
  "execute_workflow_trigger_parameters": {
    "parameters": {
      "workflowInputs": {
        "values": [
          {
            "name": "operation",
            "type": "string",
            "required": true,
            "description": "The service to use: googledrive, googledocs, or airtable"
          },
          {
            "name": "subOperation", 
            "type": "number",
            "required": true,
            "description": "The specific operation number (1-8)"
          },
          {
            "name": "searchQuery",
            "type": "string", 
            "required": false,
            "description": "Search terms for Google Drive"
          },
          {
            "name": "fileId",
            "type": "string",
            "required": false,
            "description": "Google Drive file ID"
          },
          {
            "name": "fileName",
            "type": "string",
            "required": false,
            "description": "Name for new/uploaded files"
          },
          {
            "name": "parentId",
            "type": "string",
            "required": false,
            "description": "Google Drive parent folder ID"
          },
          {
            "name": "newParentId",
            "type": "string",
            "required": false,
            "description": "Destination folder ID for move operations"
          },
          {
            "name": "newName",
            "type": "string",
            "required": false,
            "description": "New name for copied files"
          },
          {
            "name": "email",
            "type": "string",
            "required": false,
            "description": "Email address for sharing permissions"
          },
          {
            "name": "role",
            "type": "string",
            "required": false,
            "description": "Permission role: viewer, editor, owner"
          },
          {
            "name": "folderName",
            "type": "string",
            "required": false,
            "description": "Name for new folders"
          },
          {
            "name": "documentId",
            "type": "string",
            "required": false,
            "description": "Google Docs document ID or URL"
          },
          {
            "name": "title",
            "type": "string",
            "required": false,
            "description": "Title for new Google Docs"
          },
          {
            "name": "text",
            "type": "string",
            "required": false,
            "description": "Text content to insert into documents"
          },
          {
            "name": "index",
            "type": "number",
            "required": false,
            "description": "Position to insert text in documents"
          },
          {
            "name": "baseId",
            "type": "string",
            "required": false,
            "description": "Airtable base ID"
          },
          {
            "name": "tableId",
            "type": "string",
            "required": false,
            "description": "Airtable table ID"
          },
          {
            "name": "recordId",
            "type": "string",
            "required": false,
            "description": "Airtable record ID"
          },
          {
            "name": "fields",
            "type": "object",
            "required": false,
            "description": "Airtable field data for create/update operations"
          }
        ]
      }
    }
  },
  
